<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin_computerinventorydetails"
    android:orientation="vertical"
    android:padding="100px">


    <ImageButton
        android:id="@+id/btnbackbutton"
        android:layout_width="47dp"
        android:layout_height="44dp"
        android:layout_gravity="center_vertical"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/icons8_back_button_96___" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="112dp"
        android:layout_height="112dp"
        app:srcCompat="@drawable/_701051780322"
        tools:src="@drawable/_701051780322"
        android:layout_gravity="center_horizontal"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Computer Details"
        android:gravity="center_horizontal"
        android:textStyle="bold"
        android:textSize="34dp"/>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/recyclerview"/>


</LinearLayout>